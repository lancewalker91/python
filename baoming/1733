<script>
$(document).ready(function(e){
	var source= "reg";
	$("#regcheck1,#regcheck2,#regcheck3,#regcheck4,#regcheck5,#regcheck6").click(function(){
		var w,h;
		if($(window).width()>800){
			w='476px';
			h='360px';
		}else{
			w='90%';
			h='400px';
		}
		if(userApi.isLogin()){
			$.post("/course.layerplan.loginedRegAjax",{
				courseid:courseId,
				classid:class_id
			},function(r){
				if (r.code == 0) {
					layer.msg('报名成功');
					setTimeout(function(){ location.reload(); },2000);
				} else if(r.code > 0){
					$(".x_error").html(r.message);
					return false;
				} else if (r.code == -2) {
					var index = layer.load(2);
					$.post("/course/pay/check", {
						classId: class_id,
						cid: courseId
					}, function (r) {
						layer.close(index);
						if (r.code == 0) {
							location.href = "/course/info/pay/"+ r.result.uniqueOrderId;
						} else {
							//layer.msg(r.errMsg, function(){
								location.reload();
							//});
						}
					}, "json");
				} else if (r.code == -5 ) {
					layer.msg('名额已满');
					setTimeout(function(){ location.reload(); },2000);
				} else if (r.code == -6 ) {
					layer.msg('你已经报了别的班啦');
					setTimeout(function(){ location.reload(); },2000);
				} else if (r.code == -3 || r.code == -4) {
					location.reload();
				} else {
					layer.msg('未知错误', function(){
						location.reload();
					});
				}
			},"json");

			//如果未登录
		}else{
			if (userApi.isWeiXin()) {
				location.replace("/weixin.user.login/?url=//"+location.hostname+"/course.info.show/"+courseId);
			} else {
            if($(window).width() < 800){
                location.replace("/site.main.login/?url=//"+location.hostname+"/course.plan.play/1733")
            }else{
				layer.open({
                    title:false,
					type: 2,
					area: [w, h],
					shadeClose: true,
					content: ['/layer.main.userLogin/'+courseId+'/'+class_id+'/'+source, 'no']
				});
                }
			}
			//弹登录层
		}

		return false;
	});

});
</script>
